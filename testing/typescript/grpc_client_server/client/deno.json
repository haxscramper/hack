{
  "tasks": {
    "dev": "deno run --allow-env --allow-net --watch main.ts",
    "gen": "mkdir -p protobuf && protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=generated --proto_path=../server org.proto"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@grpc/grpc-js": "npm:@grpc/grpc-js",
    "google-protobuf": "npm:google-protobuf@^3.21.4",
    "protobufjs": "npm:protobufjs",
    "protoc-gen-ts": "npm:protoc-gen-ts@^0.8.7"
  },
  "nodeModulesDir": "auto"
}
